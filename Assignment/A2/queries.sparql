PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?wound ?type ?bleedingTime ?location
WHERE {
    ?wound rdf:type ?type .
    ?wound hasBleedingDuration ?bleedingTime .
    ?wound hasLocation ?location .
    FILTER (?bleedingTime > 20)
    FILTER (?type IN (owl:OpenWound, owl:Laceration, owl:Avulsion))
}